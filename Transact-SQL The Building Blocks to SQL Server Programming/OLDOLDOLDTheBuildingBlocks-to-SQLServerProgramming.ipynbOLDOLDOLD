        {
            "cell_type": "code",
            "source": [
                "SELECT  BusinessEntityID ,\n",
                "        SickLeaveHours ,\n",
                "        VacationHours\n",
                "FROM    AdventureWorks2017.HumanResources.Employee\n",
                "WHERE   SickLeaveHours > 68\n",
                "        AND VacationHours > 98 ;"
            ],
            "metadata": {
